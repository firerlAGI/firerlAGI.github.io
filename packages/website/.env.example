# GitHub Token for fetching stats
# Get it from: https://github.com/settings/tokens
# Select public_repo scope for basic stats
GITHUB_TOKEN=your_github_token_here

# Deepseek API Key for AI Assistant
# Get it from: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Trae API Key for Trae integration
# Get it from: https://trae.ai/settings/api-keys
TRAE_API_KEY=your_trae_api_key_here
